generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TestInscription {
  id        Int      @id @default(autoincrement())
  nom       String
  prenom    String
  numero    String
  niveau    String
  document  String
  audio     String
  statut    String   @default("En attente")
  createdAt DateTime @default(now())
}
